{
  "name": "react-looper",
  "version": "0.2.8",
  "description": "Audio Looper for React",
  "main": "./cjs/looper.js",
  "module": "./esm/looper.js",
  "types": "./esm/looper.d.ts",
  "scripts": {
    "prebuild": "yarn clean",
    "prepublish": "yarn build",
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc --outDir cjs --module commonjs",
    "build:esm": "tsc --outDir esm --module esnext"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/jest": "^23.3.8",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "parcel": "^1.10.3",
    "prepublish": "^2.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^16.6.0"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "sideEffects": false,
  "author": "Peter Shershov <petershershov@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:PeterShershov/react-looper.git",
  "homepage": "https://github.com/PeterShershov/react-looper"
}
